	year = {2015},
	note = {ZSCC: 0000931 
Publisher: Public Library of Science; https://web.archive.org/web/20211108231052/https://journals.plos.org/plosone/article?id=10.1371\%2Fjournal.pone.0127502},
	keywords = {Citation analysis, Bibliometrics, Scientific publishing, Social sciences, archived, Economics, Libraries, Medical humanities, Medical journals},
	pages = {e0127502},
	file = {Full Text PDF:/Users/rosariorogel/Zotero/storage/K5Y2687I/Larivière et al. - 2015 - The Oligopoly of Academic Publishers in the Digita.pdf:application/pdf;Snapshot:/Users/rosariorogel/Zotero/storage/VLLRSJFF/article.html:text/html},
