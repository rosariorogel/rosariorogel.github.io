@article{wood_sparking_2018,
	title = {Sparking a {Transition}, {Unmasking} {Confusion}: {An} {Empirical} {Study} of the {Benefits} of a {Writing} {Center} {Workshop} about {Patchwriting}},
	volume = {37},
	issn = {0889-6143},
	shorttitle = {Sparking a {Transition}, {Unmasking} {Confusion}},
	url = {http://www.jstor.org/stable/26537364},
	abstract = {Students' misunderstanding of faculty expectations for paraphrase has been empirically demonstrated, and many writing centers conduct workshops to help students adopt better strategies for work with sources. However, little empirical research supports the effectiveness of such efforts. For this study, researchers examined students' attempts to paraphrase before and after a 45-minute workshop presented by an undergraduate peer tutor in several sections of an introductory political science course. Our findings demonstrate that the workshop did help students improve both their understanding of what is expected of them and their attempts to paraphrase. The average score for language increased from 3.11 in the pretest to 3.86 on a 5-point scale in the posttest (𝑛=107, 𝑝≤.001). However, as many students improved at avoiding patchwriting, the quality of their representation of an idea from a source appeared to decline; ideas scores dropped after the workshop from 3.36 to 3.03 (𝑛=107, 𝑝≤.001). The drop in scores for ideas supports concerns that students' practice of patchwriting may serve to mask problems with their reading comprehension. These findings suggest that writing centers can effectively partner with faculty to help students read and work more thoughtfully with source material and therefore engage more fully with course material.},
	number = {1},
	urldate = {2021-04-21},
	journal = {The Writing Center Journal},
	author = {Wood, Jessa and Roggenbuck, Ted and Doerschler, Peter and Hicks, Megan},
	year = {2018},
	note = {Publisher: Writing Center Journal},
	pages = {101--130},
}

