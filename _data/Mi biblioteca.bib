@article{rogel-salazar_revistas_2012,
	title = {Revistas académicas de arte en bases de datos bibliográficas: disponibilidad en acceso abierto y en bibliotecas de tres instituciones mexicanas},
	copyright = {CC0 1.0 Universal Public Domain Dedication},
	url = {https://ornitorrincotachado.uaemex.mx/article/view/4616},
	abstract = {Objetivo: Identificar la presencia de las revistas académicas de arte en las principales bases de datos bibliográficas (WoS y Scopus a nivel internacional, así como Latindex, Scielo y Redalyc en el ámbito Iberoamericano). Método: Se describe la participación de los títulos vigentes en 2011 por país, idioma y ámbito temático. En el caso de las revistas registradas en el Arts \& Humanities Citation Index –a\&hci, Thomson Reuters isi– se identificó su tipo de acceso (abierto o por suscripción), así como su disponibilidad en tres bibliotecas de instituciones mexicanas vinculadas a las disciplinas artísticas: la Universidad Nacional Autónoma de México (unam), el Centro Nacional de las Artes (cna) y la Universidad Autónoma del Estado de México (uaem). Resultados: No se registra ninguna revista latinoamericana de arte en el a\&hci, su disponibilidad en acceso abierto es limitada (11.3\%), así como también su disponibilidad en las bibliotecas analizadas, con una variación del 20 al 6\% en los casos analizados.},
	language = {Español},
	number = {1},
	journal = {El Ornitorrinco Tachado. Revista de Artes Visuales},
	author = {Rogel-Salazar, Rosario and Aguado-López, Eduardo and Martínez-Dominguez, Néstor and Lujano-Vilchis, Ivonne and Monroy-Vilchis, Trinidad},
	year = {2012},
	note = {Publisher: Universidad Autónoma del Estado de México},
	pages = {23--43},
}

@inproceedings{aguado-lopez_cooperacion_2011,
	address = {La Habana, Cuba},
	title = {Cooperación iberoamericana para el {Acceso} {Abierto} a revistas de {Ciencias} {Sociales}: el caso {CLACSO}-{Redalyc}},
	copyright = {CC0 1.0 Universal Public Domain Dedication},
	url = {https://hdl.handle.net/2142/25877},
	abstract = {Las ciencias sociales de América Latina y el Caribe contribuyen significativamente al desarrollo de nuevos conocimientos de interés para el desarrollo sustentable de la región. La difusión de esos nuevos conocimientos se ve limitada por la falta de recursos para publicar y hacer visible los resultados de investigaciones. Se presenta el caso de la alianza entre el Consejo Latinoamericano de Ciencias Sociales (CLACSO) y la Red de Revistas Científicas de América Latina y el Caribe, España y Portugal (REDALYC) para el desarrollo de una colección de revistas de ciencias sociales en acceso abierto, como estrategia que permite mejorar la visibilidad, accesibilidad y evaluación de los resultados de investigaciones. Y se describe el rol de los editores de las revista y de las bibliotecas en el desarrollo y actualización de las colecciones digitales de revistas.},
	booktitle = {{IFLA} 2011, {International} {Federation} of {Library} {Associations}, {Social} {Science} {Libraries} {Section}, {Satellite} {Conference}, {Social} {Science} {Libraries}: {A} {Bridge} to {Knowledge} for {Sustainable} {Development}},
	author = {Aguado-López, Eduardo and Rogel-Salazar, Rosario and Babini, Dominique and López, Fernando and González, Jessica},
	year = {2011},
}

