	file = {Zenodo Full Text PDF:/Users/rosariorogel/Zotero/storage/W2TFU6ZE/Repiso y Delgado-Vázquez - 2023 - FALLEN JOURNALS 2023. Implicaciones para la cienci.pdf:application/pdf},
