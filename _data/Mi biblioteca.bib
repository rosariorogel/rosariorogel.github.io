@article{alcaraz_martinez_omeka_2022,
	title = {Omeka {S} como alternativa para el desarrollo de colecciones digitales y proyectos de humanidades digitales},
	issn = {1575-5886},
	url = {https://bid.ub.edu/es/48/alcaraz.htm},
	doi = {10.1344/BiD2022.48.06},
	abstract = {Objetivos: se presenta una descripción pormenorizada de las principales características y funcionalidades de Omeka S, un sistema de gestión de contenidos de software libre para la creación de repositorios y bibliotecas digitales, así como ampliamente utilizado en proyectos de humanidades digitales de diversa índole.{\textless}br /{\textgreater} Metodología: el análisis de las características de Omeka S se ha realizado a partir de una revisión de la literatura publicada, la información disponible en el sitio web y la documentación oficial de la aplicación, así como mediante su instalación, uso y análisis. La descripción de la aplicación se complementa con una recopilación de diversos casos de uso de carácter internacional en el área de las humanidades digitales que muestran la versatilidad de la herramienta. Finalmente, se ofrece una comparativa con otras cinco aplicaciones de software libre disponibles en el mercado: Omeka Classic, CollectiveAccess, Mukurtu, ResearchSpace y Arches.{\textless}br /{\textgreater} Resultados: entre los principales puntos fuertes de Omeka S destaca su facilidad de instalación, mantenimiento y uso, su arquitectura multisitio, su facilidad para integrar vocabularios de la web semántica como ontologías, su flexibilidad para crear plantillas de descripción para los objetos digitales, su libertad para el diseño de las páginas que conforman la interfaz pública o su integración nativa con otros sistemas de gestión de repositorios, como DSpace o Fedora Commons, pudiéndose implementar de esta manera como plataforma para la gestión y publicación de recursos derivados del repositorio, o como capa de acceso público a sus objetos digitales, respectivamente. Por otro lado, la gestión de un proyecto con Omeka S requiere de un mayor esfuerzo inicial derivado de su gran flexibilidad. Finalmente, el sistema se muestra menos competente en el acceso a la base de datos, el cual no se puede realizar de manera nativa a través de consultas SPARQL.},
	language = {ca},
	number = {48},
	urldate = {2023-11-21},
	journal = {BiD: textos universitaris de biblioteconomia i documentació},
	author = {Alcaraz Martínez, Rubén},
	month = jun,
	year = {2022},
}

@article{royo_segarra_licencias_2018,
	title = {Las licencias {Creative} {Commons} como modelo normativo alternativo de protección de la propiedad intelectual: descripción y análisis de su integración en el {Derecho} español},
	shorttitle = {Las licencias {Creative} {Commons} como modelo normativo alternativo de protección de la propiedad intelectual},
	url = {https://typeset.io/papers/las-licencias-creative-commons-como-modelo-normativo-4wdxy8eev3},
	abstract = {espanolLa irrupcion de las tecnologias de la informacion y la comunicacion y el auge de la sociedad del conocimiento han puesto en evidencia que el modelo espanol tradicional de proteccion de los derechos de autor resulta insuficiente para responder a las nuevas necesidades sociales. Ante esta situacion, han surgido filosofias y modelos alternativos, como son el copyleft y las licencias Creative Commons, los cuales no deben entenderse como una vision opuesta al copyright, sino mas bien como alternativa conforme a la Ley, en la que encuentran cabida a partir de las facultades de disposicion que esta otorga a los titulares de derechos de autor. Ahora bien, las peculiares caracteristicas que el ordenamiento espanol otorga a los derechos de autor hacen que la insercion de estos nuevos modelos resulte, a veces, complicada. En este contexto, el presente trabajo realiza un analisis del modelo de licencias Creative Commons, prestando especial atencion a los puntos que resultan mas controvertidos. EnglishThe irruption of new information and communication technologies and the rise of knowledge societies have provided evidence that the current Spanish legal system on copyright protection is not able to respond to some new social needs. In this context, new ideas and alternative models on this matter, such as the copyleft philosophy or Creative Commons licenses, have emerged. These concepts must not be understood as opposed to traditional copyright protection, but as lawful alternatives, since the Law provides the authors with rights of disposal over their works and their exploitation. However, the interaction with our legal framework is sometimes difficult, due to the unique characteristics of our copyright system. In this situation, this paper aims to analyse the Creative Commons licenses within the context of Spanish law, focusing on the most controversial aspects.},
	language = {en},
	number = {50},
	urldate = {2023-11-21},
	author = {Royo Segarra, Rita},
	month = jan,
	year = {2018},
	note = {Publisher: Wolters Kluwer},
	pages = {6},
	file = {Snapshot:/Users/rosariorogel/Zotero/storage/9LLFCKEU/las-licencias-creative-commons-como-modelo-normativo-4wdxy8eev3.html:text/html},
}

