@inproceedings{aguado-lopez_metricas_2013,
	address = {Facultad de Ciencias,Universidad Nacional Autónoma de México},
	title = {Métricas alternativas para la producción científica en revistas de acceso abierto redalyc.org, 2005-2011},
	copyright = {CC0 1.0 Universal Public Domain Dedication},
	url = {https://conference.pkp.sfu.ca/index.php/pkp2013/pkp2013/paper/view/442},
	abstract = {El presente documento es un acercamiento a una de las primeras propuestas concretas del Laboratorio de Cienciometría redalyc.org-fractal (LabCrf ®) para la generación de métricas alternativas que buscan describir el estado de la producción científica en revistas iberoamericanas de acceso abierto en redaly.org para el periodo 2005-2011. Metodología: exponer un modelo de análisis basado en entidades de producción y comunicación que aplican un conjunto de indicadores con el fin de construir el “estado del arte” para la producción científica en Iberoamérica, a partir de los Perfiles de Producción Científica que se desarrollan por país, institución y área de conocimiento. Resultados: las métricas alternativas propuestas por redalyc.org hacen visibles las contribuciones científicas del mundo en revistas iberoamericanas ‘perdidas’ en otras bases de datos, al tiempo que presentan los datos de producción y colaboración de 13,414 instituciones identificando la característica de su contribución: en publicaciones institucionales, nacionales y/o de otros países. Estas métricas alternativas permiten completar la producción regional, sobre todo en las Ciencias Sociales ya que redalyc.org cuenta con más de 500 revistas de éstas disciplinas.},
	booktitle = {{PKP} {International} {Scholarly} {Publishing} {Conferences}},
	author = {Aguado-López, Eduardo and Rogel-Salazar, Rosario and Becerril-García, Arianna},
	year = {2013},
}

@article{becerril-garcia_modelo_2012,
	title = {De un modelo centrado en la revista a un modelo centrado en entidades: la publicación y producción científica en la nueva plataforma {Redalyc}.org},
	volume = {40},
	copyright = {CC0 1.0 Universal Public Domain Dedication},
	issn = {0210-2773},
	url = {http://hdl.handle.net/11162/4191},
	abstract = {La presentación de información que recoja la actividad investigadora de países e instituciones ha sido liderada por índices comerciales y bases de datos que conforman lo que se conoce como 'ciencia de corriente principal'. Sin embargo, existe una necesidad latente por el uso de métricas alternativas que permitan conocer mejor las formas de comunicación científica, particularmente en la región de Iberoamérica, ya que su participación se ve sub-representada en la corriente principal. Por ello, Redalyc, como sistema de información científica regional, construyó una nueva plataforma que reconoce la contribución de todas las instituciones sin distingo y propone un modelo que expande el enfoque centrado en la revista -que venía operando- a autores, instituciones, países y áreas del conocimiento. Este modelo es descrito con la metodología Entidad-Relación de bases de datos, en el cual, cada elemento distinto actúa como una entidad con dos componentes principales: una colección y un perfil de actividad científica. Así la región cuenta con una plataforma tecnológicamente sólida y de acceso abierto que permite consultar el contenido de la producción científica del mundo en revistas Iberoamericanas (Redalyc.org) a través de una solución extensiva a las contribuciones en distintos niveles con un universo delimitado y completo.},
	number = {2},
	journal = {Aula Abierta},
	author = {Becerril-García, Arianna and Aguado-López, Eduardo and Rogel-Salazar, Rosario and Garduño-Oropeza, Gustavo and Zúñiga-Roca, María Fernanda},
	year = {2012},
	note = {Publisher: España: ICE. Universidad de Oviedo},
	pages = {53--64},
}

