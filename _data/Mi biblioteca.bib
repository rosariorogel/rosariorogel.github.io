@techreport{vega_manco_methodological_2022,
	title = {Methodological landscape of qualitative works on open science},
	url = {https://mediarxiv.org/ac2zd/},
	abstract = {This review aims to examine the characteristics of the qualitative articles on open science. The bibliographic searches were made in Scopus, Web of Science, Semantic Scholar and Google Scholar using open science as a keyword. A total of 28 relevant articles were finally selected. It was found that the most used techniques are interviews and document analysis. The type of research design usually is descriptive and exploratory. There is a lack of a formal sampling strategy in the majority of the articles, although when mentioned the most common types are snowball, purposive, criterion, convenience and self-selective. The study sample is usually small in the case of interviews and case studies. The limitations are related to two issues: specificities of exploratory research and generalization. Also, the majority of article corpuses do not publish their research data. In conclusion, the chosen methodologies are rather traditional qualitative methods. For instance, none of the revised papers in this corpus used digital qualitative methods. The usage of only descriptive and exploratory research design may indicate that open science is still a fairly young field of research. Finally, qualitative research on open science is frequently very localized.},
	language = {en-us},
	urldate = {2022-05-17},
	institution = {MediArXiv},
	author = {Vega Manco, Alejandra},
