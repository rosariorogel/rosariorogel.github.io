@article{tsai_promises_2016,
	title = {Promises and pitfalls of data sharing in qualitative research},
	volume = {169},
	issn = {0277-9536},
	url = {http://www.sciencedirect.com/science/article/pii/S0277953616304269},
	doi = {10.1016/j.socscimed.2016.08.004},
	abstract = {The movement for research transparency has gained irresistible momentum over the past decade. Although qualitative research is rarely published in the high-impact journals that have adopted, or are most likely to adopt, data sharing policies, qualitative researchers who publish work in these and similar venues will likely encounter questions about data sharing within the next few years. The fundamental ways in which qualitative and quantitative data differ should be considered when assessing the extent to which qualitative and mixed methods researchers should be expected to adhere to data sharing policies developed with quantitative studies in mind. We outline several of the most critical concerns below, while also suggesting possible modifications that may help to reduce the probability of unintended adverse consequences and to ensure that the sharing of qualitative data is consistent with ethical standards in research.},
	urldate = {2018-06-05},
	journal = {Social Science \& Medicine},
	author = {Tsai, Alexander C. and Kohrt, Brandon A. and Matthews, Lynn T. and Betancourt, Theresa S. and Lee, Jooyoung K. and Papachristos, Andrew V. and Weiser, Sheri D. and Dworkin, Shari L.},
	month = nov,
	year = {2016},
	keywords = {Confidentiality, Data sharing, Ethnography, Mixed methods, Qualitative research, Reproducibility, Transparency},
	pages = {191--198},
