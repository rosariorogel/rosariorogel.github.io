	note = {Publisher: British Medical Journal Publishing Group
Section: Research},
	pages = {m1328},
	file = {Full Text PDF:/Users/rosariorogel/Zotero/storage/FZKCTWNQ/Wynants et al. - 2020 - Prediction models for diagnosis and prognosis of covid-19 systematic review and critical appraisal.pdf:application/pdf},
