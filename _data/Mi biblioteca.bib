@article{noauthor_laberintos_nodate,
	title = {Los laberintos de la descentralización educativa},
	url = {https://zenodo.org/records/10278256},
	doi = {10.5281/zenodo.10278256},
	abstract = {Este estudio aborda la descentralización como un fenómeno que configura la pluralidad, empleando la metáfora del laberinto para explorar sus manifestaciones, particularmente en el ámbito de la descentralización educativa en México, con enfoque en la educación básica. La investigación se divide en dos secciones: un análisis conceptual y la aplicación de este enfoque al caso específico del Estado de México.El propósito central es proporcionar un conjunto de estrategias para examinar la descentralización educativa, destacando la complejidad de este proceso dinámico y plural. Se utiliza una cartografía conceptual para explorar categorías taxonómicas y analizar cómo diversos discursos atribuyen obstáculos o posibilidades a la descentralización. La taxonomía resultante abarca categorías proyectuales, representacionales, gestionales y territoriales.El análisis teórico destaca siete contribuciones fundamentales, como la adopción de una perspectiva transdisciplinaria y el reconocimiento del lenguaje político como un factor crucial. Se enfoca en el territorio como soporte y considera la descentralización como parte de cambios en la organización del territorio, reconociendo que la descentralización es un proceso dinámico que no corresponde a un solo discurso.La segunda parte del estudio se centra en la descentralización educativa en el Estado de México, destacando su complejidad debido a características únicas en términos económicos, políticos y sociales. Se revela que, en este contexto, la descentralización no descentraliza la toma de decisiones educativas, sino más bien la administración y financiamiento, generando desafíos significativos, especialmente en la transferencia de fondos para el gasto educativo en las entidades.La investigación resalta las contradicciones y desafíos inherentes a la descentralización educativa, ofreciendo un enfoque analítico novedoso y proponiendo categorías taxonómicas para comprender mejor este fenómeno en el contexto mexicano, específicamente en el Estado de México.},
	language = {en},
	urldate = {2023-12-06},
	file = {Full Text PDF:/Users/rosariorogel/Zotero/storage/WD8RIL4P/Los laberintos de la descentralización educativa.pdf:application/pdf},
}

